{
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
          "src": "/(.*)",
          "dest": "index.js"
        }
    ],
    "installCommand": "pnpm install --no-frozen-lockfile",
    "buildCommand": "pnpm install --no-frozen-lockfile",
    "outputDirectory": "."
}
